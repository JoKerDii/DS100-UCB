    "# path to directory containing data\n",
    "dsDir = Path('data')\n",
    "\n",
    "bus = pd.read_csv('data/businesses.csv',encoding='ISO-8859-1')\n",
    "ins = pd.read_csv('data/inspections.csv',encoding='ISO-8859-1')\n",
    "vio = pd.read_csv('data/violations.csv',encoding='ISO-8859-1')"
   ]
  },
