      "text/plain": [
       "                               name\n",
       "postal_code_5                      \n",
       "94102          [TURK & LARKIN DELI]\n",
       "94103            [THE CHENNAI CLUB]\n",
       "94104                       [PLOUF]\n",
       "94105                  [JUICE SHOP]\n",
       "94107              [BAYSIDE MARKET]"
      ]
     },
